{
    "name": "nightschool-studio/blank-woocommerce-theme-installer",
    "description": "Nightschool Studio WP Install for WooCommerce Projects",
    "type": "package",    
    "require": {
        "afragen/github-updater": "^9",
        "composer/installers": "^1"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "license": "none",
    "authors": [
        {
            "name": "Magalie Chetrit",
            "email": "magaliechetrit@nightschool.studio"
        }
    ],
    "minimum-stability": "dev"
}
